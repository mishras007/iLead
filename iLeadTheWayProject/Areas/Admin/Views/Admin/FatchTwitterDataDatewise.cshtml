
@{
    ViewBag.Title = "FatchTwitterDataDatewise";
    Layout = "~/Areas/Admin/Views/Shared/_Admin.cshtml";
}
<div class="col-md-6 col-md-offset-3">
    <div class="panel panel-default">
        <div class="panel-heading" style="text-align: center">
            <strong class="">Select Date </strong>
        </div>
        @using (Html.BeginForm("FatchTwitterDataDatewise", "Admin", null, FormMethod.Post))
        {
            @Html.AntiForgeryToken()
            <div class="panel-body form-horizontal ">
                <div class="form-group">
                    <div id="Error"></div>
                    <label class="col-sm-6 control-label"> Select Any Two Dates </label>
                </div>
                <div class="form-group">
                    <div id="Error"></div>
                    <label class="col-sm-3 control-label">Select Date </label>
                    <div class="col-sm-4 col-md-offset-1">
                        <input class="form-control ui-datepicker-next-hover" id="d1" name="d1" placeholder="Start Date" required="" type="date">
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-sm-3 control-label">End Date</label>
                    <div class="col-sm-4 col-md-offset-1">
                        <input class="form-control ui-datepicker-next-hover" id="d2" name="d2" placeholder="End Date" required="" type="date">
                    </div>
                </div>
                <div class="form-group last">
                    <div class="col-sm-offset-4 col-sm-8">
                        <input type="submit" id="btnSubmit" name="btnSubmit" value="Submit" class="btn btn-success" />
                      
                    </div>
                </div>
            </div>
        }
    </div>
    @if (ViewBag.SuccessMessage != null)
    {
        <b style="color: blue">@ViewBag.SuccessMessage</b>
    }
</div>

