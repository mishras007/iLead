@{
    Layout = "~/Areas/Admin/Views/Shared/_Admin.cshtml";
    @model List<EntityProject.RegistrationUser>
}
<div class="col-lg-12">
    @if (ViewBag.ErrorMessage != null)
    {
        @ViewBag.ErrorMessage
    }
    @if (ViewBag.SuccessMessage != null)
    {
        @ViewBag.SuccessMessage
    }
</div>
<div class="col-lg-offset-10 col-md-2">
    @using (Html.BeginForm("ExportData", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <input type="submit" class="btn btn-primary " name="Export" id="Export" value="Export All Data" />
    }
</div>
<div class="col-lg-12 ">
    <br />
    <table id="RegisterUser" class="table table-hover table-striped table-bordered table-responsive">
        <thead>
            <tr>
                <th>Index No</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Mobile No</th>
                <th>Email Id</th>
                <th>State</th>
                <th>City</th>
                @*<th><i class="fa fa-desktop fa-fw"></i></th>
                    <th><i class="fa fa-edit fa-fw"></i></th>
                    <th><i class="fa fa-trash-o fa-fw"></i></th>*@
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.RegistrationUserId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FirstName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.LastName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MobileNumber)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EmailId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.State)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.City)
                    </td>
                    @*<td>@Html.ActionLink("View Details", "Viewuser", new { id = item.AdminDetailId }, new { id = "lnkDetail" })</td>
                        <td>@Html.ActionLink("Edit", "EditUser", new { id = item.AdminDetailId }, new { id = "lnkEdit" }) </td>
                        <td>@Html.ActionLink("Delete", "DeleteUser", new { id = item.AdminDetailId }, new { onclick = "return confirm('Are you sure you wish to delete this Record?');" })</td>*@
                </tr>
            }
        </tbody>
    </table>
</div>
